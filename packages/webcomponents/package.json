{
  "name": "@rws-air/webcomponents",
  "version": "7.0.1",
  "description": "Standard Webcomponents for AIR projects",
  "author": "@rws-air",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "homepage": "https://github.com/RWS-NL/air-node-packages/tree/master/packages/webcomponents#readme",
  "scripts": {
    "test": "yarn lint",
    "test:modules": "react-scripts test",
    "posttest:modules": "git checkout tsconfig.json",
    "lint": "concurrently \"yarn lint:js\" \"yarn lint:scss\"",
    "lint:js": "eslint src --fix --ext .ts,.tsx",
    "lint:scss": "stylelint --fix ./src/styles/*.scss ./src/**/*.scss",
    "prebuild": "yarn lint && CI=true yarn test:modules",
    "build": "rollup -c rollup.config.js",
    "start": "rollup -wc rollup.config.js",
    "start:local": "pushd local-sample && yarn install && yarn start",
    "modules": "rm -rf node_modules yarn.lock local-sample/node_modules local-sample/yarn.lock && yarn && pushd local-sample && yarn && popd"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@material-ui/core": ">=4",
    "@material-ui/icons": ">=4",
    "classnames": ">=2",
    "lodash.debounce": ">=4",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RWS-NL/air-node-packages.git",
    "directory": "packages/webcomponents"
  },
  "bugs": {
    "url": "https://github.com/RWS-NL/air-node-packages/issues"
  },
  "keywords": [
    "react",
    "material-ui",
    "rws-air",
    "rws-nl",
    "rws",
    "air",
    "typescript",
    "ts"
  ]
}
