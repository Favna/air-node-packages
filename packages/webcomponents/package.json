{
  "name": "@rws-air/webcomponents",
  "version": "3.0.0",
  "description": "Standard Webcomponents for AIR projects",
  "author": "@rws-air",
  "license": "AGPL-3.0-or-later",
  "repository": "RWS-NL/air-node-packages",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "lint": "concurrently \"yarn lint:js\" \"yarn lint:scss\"",
    "lint:js": "eslint src --fix --ext .ts,.tsx",
    "lint:scss": "stylelint --fix ./src/styles/*.scss ./src/**/*.scss",
    "build": "rollup -c rollup.config.js",
    "start": "yarn build -w",
    "start:local": "pushd local-sample && yarn install && yarn start",
    "prepublishOnly": "yarn lint && yarn test:ci && git co tsconfig.json && yarn build",
    "modules": "rm -rf node_modules yarn.lock local-sample/node_modules local-sample/yarn.lock && yarn && pushd local-sample && yarn && popd"
  },
  "peerDependencies": {
    "@material-ui/core": ">=4",
    "@material-ui/icons": ">=4",
    "classnames": ">=2",
    "lodash.debounce": ">=4",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist"
  ],
  "gitHead": "311e33a3661511bc9a4267ef6f6ec3b4ee0d6333"
}
