{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "build",
    "rootDir": "src",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "noEmit": true,
    "downlevelIteration": true,
    "jsx": "react"
  },
  "include": ["src"],
  "exclude": ["src/__tests__", "src/setupTests.ts"]
}
