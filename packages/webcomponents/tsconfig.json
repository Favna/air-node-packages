  "compilerOptions": {
    "outDir": "build",
    "rootDir": "src",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "noEmit": true,
    "downlevelIteration": true,
    "jsx": "react"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/setupTests.ts",
    "src/__tests__"
  ]
}