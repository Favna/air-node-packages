{
  "name": "@rws-nl/eslint-config",
  "version": "1.0.0",
  "description": "Standard ESLint config for RWS-NL ReactJS projects written in TypeScript",
  "author": "@rws-nl",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "homepage": "https://github.com/rws-nl/packages/eslint-config#readme",
  "scripts": {
    "lint": "eslint src --fix --ext .ts,.tsx",
    "test": "yarn lint",
    "prebuild": "yarn lint",
    "build": "rollup -c rollup.config.ts",
    "start": "yarn build -w",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rws-nl/air-node-packages.git"
  },
  "bugs": {
    "url": "https://github.com/rws-nl/air-node-packages/issues"
  },
  "keywords": [
    "eslint",
    "config",
    "rws-nl",
    "typescript",
    "ts"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "peerDependencies": {
    "eslint": "^5 || ^6"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1",
    "@typescript-eslint/parser": "^1"
  }
}
